# AVX Copilot Environment Configuration

# REQUIRED
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# OPTIONAL - For Audio Transcription
OPENAI_API_KEY=your_openai_api_key_here

# OPTIONAL - For Claude AI (if not set, uses mock responses)
CLAUDE_API_KEY=your_claude_api_key_here

# OPTIONAL - For GitHub Integration
GITHUB_TOKEN=your_github_token_here

# OPTIONAL - For Supabase (v2 features)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Development
NODE_ENV=production
PORT=3000

# Test User (for automated tests)
TEST_USER_ID=your_telegram_user_id_here
